{
  "name": "lst/laravel-queue-rocketmq",
  "description": "Rocketmq driver for Laravel Queue. Supports Laravel Horizon",
  "license": "MIT",
  "authors": [
    {
      "name": "lst",
      "email": "lst00190@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "illuminate/database": "5.7.*|5.8.*",
    "illuminate/support": "5.7.*|5.8.*",
    "illuminate/queue": "5.7.*|5.8.*",
    "aliyunmq/mq-http-sdk": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "illuminate/events": "5.7.*|5.8.*",
    "mockery/mockery": "^1.0",
    "laravel/horizon": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "lst\\LaravelQueueRocketMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "lst\\LaravelQueueRocketMQ\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0-dev"
    },
    "laravel": {
      "providers": [
        "lst\\LaravelQueueRocketMQ\\LaravelQueueRocketMQServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
